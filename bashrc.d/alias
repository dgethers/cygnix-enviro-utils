#!source
alias merge="svn merge --accept=postpone"
#alias svnup="svn update --accept=postpone"
alias sshes='grep Host ~/.ssh/config'
alias hosts='grep Host ~/.ssh/config'
alias ping=/cygdrive/c/Windows/system32/ping
alias hello='echo hello'

# alias less='less -r'                          # raw control characters
 alias whence='type -a'                        # where, of a sort
 alias grep='grep --color'                     # show differences in colour
 alias egrep='egrep --color=auto'              # show differences in colour
 alias fgrep='fgrep --color=auto'              # show differences in colour
 alias grepp='grep -P'

# Some shortcuts for different directory listings
# alias ls='ls -hF --color=tty'                 # classify files in colour
 alias dir='ls --color=auto --format=vertical'
 alias vdir='ls --color=auto --format=long'
 alias ll='ls -l'                              # long list
 alias la='ls -A'                              # all but . and ..
 alias l='ls -CF'     

alias jmx-ps="x1args cygstart --args http://nyatintecn0{1..8}.fxall.com:8085 http://nyatintecn0{3,4}.fxall.com:8088"
alias jmx-gs="x1args cygstart --args http://nyatintecn0{1..8}.fxall.com:8082"
alias jmx-cs="x1args cygstart --args http://nyatintecn0{1,2}.fxall.com:8100"
alias svn-summ='svn diff --diff-cmd diff -x -q'
alias sshl="ssh-launch"
